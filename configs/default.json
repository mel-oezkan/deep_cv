{

    "env": {
        "gpu_limit": 3144
    },

    "data": {
        "dataset_name": "reduced_data",
        "in_shape": [128, 128, 4],
        "out_shape": [128, 128, 1],
        "batch_size": 16,
        "augmentation": [
            "normalize",
            "flip_right",
            "flip_left"
        ],
        "debug": true
    },

    "training": {
        "epochs": 12,
        "model_type": "unet",
        "lr-rate": 1e-5,
        "callbacks": [],
        "optimizer": "adam",
        "loss": "hybrid_loss"
    }
}